{
    "name": "endroid/google-analytics-bundle",
    "description": "Endroid Google Analytics for Symfony",
    "keywords": ["endroid", "google", "analytics", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidGoogleAnalyticsBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.9",
        "phpunit/phpunit": "~4.7",
        "symfony/var-dumper": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\GoogleAnalyticsBundle\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
